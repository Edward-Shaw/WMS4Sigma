    	<dependency>
  			<groupId>org.slf4j</groupId>
  			<artifactId>slf4j-log4j12</artifactId>
  			<version>1.5.8</version>
  		</dependency>

    	<dependency>
    		<groupId>apicommon</groupId>
    		<artifactId>apicommon</artifactId>
    		<version>0.0.8-dev</version>
    	</dependency>
